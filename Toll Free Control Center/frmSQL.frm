Private Function LoadGrid()
   On Error Resume Next
   Err.Clear
   Data1.RecordSource = txtSQL.Text
   Data1.Refresh
   If Err.Number <> 0 Then
      Beep
      MsgBox "Error #:" & Err.Number & vbCrLf & Err.Description ' "Error #: " & Err.Number & vbCrLf, Err.Description
   End If
End Function
